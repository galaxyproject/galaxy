{
  "name": "@galaxyproject/client-api",
  "version": "25.0.0-dev.0",
  "description": "A client library for the Galaxy API",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "sync-version": "node scripts/sync-version.js"
  },
  "keywords": [
    "galaxy",
    "api",
    "client"
  ],
  "author": "Galaxy Community",
  "license": "MIT",
  "dependencies": {
    "openapi-fetch": "^0.12.0"
  },
  "peerDependencies": {
    "typescript": ">=4.5.0"
  },
  "devDependencies": {
    "jsdom": "^26.1.0",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3",
    "vitest": "^3.1.3"
  }
}
