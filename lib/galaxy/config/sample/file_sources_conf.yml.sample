  label: Zenodo Sandbox (TESTING ONLY)
  url: https://sandbox.zenodo.org
  token: ${user.user_vault.read_secret('preferences/zenodo_sandbox/token')}
  public_name: ${user.preferences['zenodo_sandbox|public_name']}
